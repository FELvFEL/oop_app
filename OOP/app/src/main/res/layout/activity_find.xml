<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#393D3F"
    android:backgroundTint="#393D3F"
    tools:context=".Find">


    <ImageView
        android:id="@+id/image10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#CECBC9"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nahod_spisok" />

    <TextView
        android:id="@+id/f_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/oswald"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_image" />

    <TextView
        android:id="@+id/f_birthdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/oswald"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_name" />

    <TextView
        android:id="@+id/f_country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/oswald"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_birthdate" />

    <TextView
        android:id="@+id/f_crime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/oswald"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_country" />

    <TextView
        android:id="@+id/f_signs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="177dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="176dp"
        android:fontFamily="@font/oswald"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_crime" />

    <ImageButton
        android:id="@+id/f_image"
        android:layout_width="150sp"
        android:layout_height="200sp"
        android:layout_marginTop="152dp"
        android:background="#F3EEEB"
        android:backgroundTint="#F3EEEB"
        android:elevation="1dp"
        android:scaleType="centerCrop"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="0dp"
        android:layout_height="111dp"
        android:background="#CECBC9"
        android:elevation="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/go_main"
        android:layout_width="81dp"
        android:layout_height="42dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="336dp"
        android:background="#CDCAC8"
        android:elevation="1dp"
        android:onClick="startMain"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:srcCompat="@drawable/arrow_1" />

    <EditText
        android:id="@+id/f_input"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#000000"
        android:elevation="1dp"
        android:ems="10"
        android:fontFamily="@font/oswald"
        android:hint="Введите имя"
        android:inputType="textPersonName"
        android:textColor="#171819"
        android:textColorHighlight="#171819"
        android:textColorHint="#161718"
        android:textColorLink="#161718"
        app:layout_constraintEnd_toStartOf="@+id/f_button"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/f_button"
        android:layout_width="80dp"
        android:layout_height="63dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:background="#CDCAC8"
        android:elevation="1dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/f_input"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search" />

    <ImageView
        android:id="@+id/nahod_spisok"
        android:layout_width="440dp"
        android:layout_height="103dp"
        android:layout_marginTop="24dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/nahod_spisok" />

    <ImageView
        android:id="@+id/nahod_archive"
        android:layout_width="440dp"
        android:layout_height="103dp"
        android:layout_marginTop="24dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/nahod_archive" />

    <ImageView
        android:id="@+id/nahod_spisok1"
        android:layout_width="90dp"
        android:layout_height="59dp"
        android:layout_marginStart="312dp"
        android:layout_marginTop="24dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="@+id/nahod_spisok"
        app:layout_constraintTop_toTopOf="@+id/nahod_spisok"
        app:srcCompat="@drawable/spisok" />

    <ImageView
        android:id="@+id/nahod_archive1"
        android:layout_width="90dp"
        android:layout_height="59dp"
        android:layout_marginStart="312dp"
        android:layout_marginTop="24dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="@+id/nahod_spisok"
        app:layout_constraintTop_toTopOf="@+id/nahod_spisok"
        app:srcCompat="@drawable/archive" />

</androidx.constraintlayout.widget.ConstraintLayout>